<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500" prefWidth="1000" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
    <Label fx:id="label" layoutX="181.0" layoutY="132.0" text="username" textFill="WHITE">
        <font>
            <Font size="22.0" />
        </font>
    </Label>
    <ImageView fx:id="avatar" fitHeight="73.0" fitWidth="80.0" layoutX="76.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true" />
   <Button layoutX="55.0" layoutY="230.0" mnemonicParsing="false" onMouseClicked="#launchGame" prefHeight="41.0" prefWidth="161.0" text="Start New Game">
      <font>
         <Font size="16.0" />
      </font>
   </Button>
   <Button layoutX="55.0" layoutY="282.0" mnemonicParsing="false" onMouseClicked="#launchProfileMenu" prefHeight="41.0" prefWidth="161.0" text="Profile Menu">
      <font>
         <Font size="16.0" />
      </font>
   </Button>
   <Button layoutX="55.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#launchGameSettings" prefHeight="41.0" prefWidth="161.0" text="Game Settings">
      <font>
         <Font size="16.0" />
      </font>
   </Button>

    <stylesheets>
        <URL value="@../CSS/styles.css" />
    </stylesheets>
</AnchorPane>
